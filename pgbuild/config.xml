<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ikurski.realestateprofitcalculator" versionCode="10001" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name short="Investor calculator">Real estate profit calculator</name>
    <description>
        This calculator will help investors to assess the potential profitability of the purchased property
    </description>
    <author email="i.kurski@yahoo.com" href="www.linkedin.com/in/ikurski
">i.kurski</author>
    <plugin name="cordova-plugin-whitelist"/>
    <plugin name="cordova-plugin-splashscreen"/>
    <preference name='phonegap-version' value='cli-8.0.0' />
    <preference name="android-minSdkVersion" value="23" />
    <preference name="android-maxSdkVersion" value="26" />
    <preference name="android-targetSdkVersion" value="24" />
    <preference name="SplashScreenDelay" value="1000" />
    <preference name="FadeSplashScreen" value="false"/>
    <content src="index.html" />
    <access origin="*" />
    <platform name="android">
            <!-- ldpi    : 36x36 px
            mdpi    : 48x48 px
            hdpi    : 72x72 px
            xhdpi   : 96x96 px
            xxhdpi  : 144x144 px
            xxxhdpi : 192x192 px 

            LDPI:
                Portrait: 200x320px
                Landscape: 320x200px
            MDPI:
                Portrait: 320x480px
                Landscape: 480x320px
            HDPI:
                Portrait: 480x800px
                Landscape: 800x480px
            XHDPI:
                Portrait: 720px1280px
                Landscape: 1280x720px
            XXHDPI:
                Portrait: 960px1600px
                Landscape: 1600x960px
            XXXHDPI:
                Portrait: 1280px1920px
                Landscape: 1920x1280px -->

        <icon src="res/icon/android/ldpi.png" density="ldpi" />
        <icon src="res/icon/android/mdpi.png" density="mdpi" />
        <icon src="res/icon/android/hdpi.png" density="hdpi" />
        <icon src="res/icon/android/xhdpi.png" density="xhdpi" />
        <icon src="res/icon/android/xxhdpi.png" density="xxhdpi" />
        <icon src="res/icon/android/xxxhdpi.png" density="xxxhdpi" />

        <splash src="res/screen/android/splash-port-ldpi.png" density="port-ldpi"/>
        <splash src="res/screen/android/splash-land-ldpi.png" density="land-ldpi"/>

        <splash src="res/screen/android/splash-port-mdpi.png" density="port-mdpi"/>
        <splash src="res/screen/android/splash-land-mdpi.png" density="land-mdpi"/>

        <splash src="res/screen/android/splash-port-hdpi.png" density="port-hdpi"/>
        <splash src="res/screen/android/splash-land-hdpi.png" density="land-hdpi"/>

        <splash src="res/screen/android/splash-port-xhdpi.png" density="port-xhdpi"/>
        <splash src="res/screen/android/splash-land-xhdpi.png" density="land-xhdpi"/>

        <splash src="res/screen/android/splash-port-xxhdpi.png" density="port-xxhdpi"/>
        <splash src="res/screen/android/splash-land-xxhdpi.png" density="land-xxhdpi"/>

        <splash src="res/screen/android/splash-port-xxxhdpi.png" density="port-xxxhdpi"/>
        <splash src="res/screen/android/splash-land-xxxhdpi.png" density="land-xxxhdpi"/>

    </platform>
    <platform name="ios">
        <icon src="res/icon/ios/icon-60@3x.png" width="180" height="180" />
        <icon src="res/icon/ios/icon-60.png" width="60" height="60" />
        <icon src="res/icon/ios/icon-60@2x.png" width="120" height="120" />
        <icon src="res/icon/ios/icon-76.png" width="76" height="76" />
        <icon src="res/icon/ios/icon-76@2x.png" width="152" height="152" />
        <icon src="res/icon/ios/icon-40.png" width="40" height="40" />
        <icon src="res/icon/ios/icon-40@2x.png" width="80" height="80" />
        <icon src="res/icon/ios/icon.png" width="57" height="57" />
        <icon src="res/icon/ios/icon@2x.png" width="114" height="114" />
        <icon src="res/icon/ios/icon-72.png" width="72" height="72" />
        <icon src="res/icon/ios/icon-72@2x.png" width="144" height="144" />
        <icon src="res/icon/ios/icon-167.png" width="167" height="167" />
        <icon src="res/icon/ios/icon-small.png" width="29" height="29" />
        <icon src="res/icon/ios/icon-small@2x.png" width="58" height="58" />
        <icon src="res/icon/ios/icon-50.png" width="50" height="50" />
        <icon src="res/icon/ios/icon-50@2x.png" width="100" height="100" />
        <icon src="res/icon/ios/icon-83.5@2x.png" width="167" height="167" />
 
            <!-- 2732x2732   Default@2x~universal~anyany.png
            1278x2732   Default@2x~universal~comany.png
            1334x750    Default@2x~universal~comcom.png
            2208x2208   Default@3x~universal~anyany.png
            2208x1242   Default@3x~universal~anycom.png
            1242x2208   Default@3x~universal~comany.png -->

        <splash src="res/screen/ios/Default@2x~universal~anyany.png" /> 
        <splash src="res/screen/ios/Default@2x~universal~comany.png" />
        <splash src="res/screen/ios/Default@2x~universal~comcom.png" />
        <splash src="res/screen/ios/Default@3x~universal~anyany.png" />
        <splash src="res/screen/ios/Default@3x~universal~anycom.png" />
        <splash src="res/screen/ios/Default@3x~universal~comany.png" />
    </platform>
</widget>
